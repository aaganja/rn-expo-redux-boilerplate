{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100
      }
    ]
  },
  "plugins": ["prettier"]
}
